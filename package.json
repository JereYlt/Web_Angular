{
  "name": "pleyades-m",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --outputPath=src/locale",
    "start:en": "ng serve --aot --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en",
    "start:pt": "ng serve --aot --i18nFile=src/locale/messages.pt.xlf --i18nFormat=xlf --locale=pt",
    "start:fr": "ng serve --aot --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build:en": "ng build --aot --output-path dist/en --base-href /en/ --i18nFile=src/locale/messages.en.xlf --i18nFormat=xlf --locale=en",
    "build:fr": "ng build --aot --output-path dist/fr --base-href /fr/ --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build:pt": "ng build --aot --output-path dist/pt --base-href /pt/ --i18nFile=src/locale/messages.pt.xlf --i18nFormat=xlf --locale=pt",
    "i18n": "ng xi18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngx-translate/core": "^10.0.2",
    "angular": "^1.7.0",
    "angular-google-adsense": "^0.6.0",
    "auth0-js": "^9.7.3",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.4",
    "daypilot-pro-angular": "https://npm.daypilot.org/daypilot-pro-angular/trial/2018.2.3297.tar.gz",
    "font-awesome": "^4.7.0",
    "ng2-adsense": "^5.3.2",
    "ng2-opd-popup": "^1.1.21",
    "npm": "^6.4.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "^6.0.3",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.1.7",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^1.12.4",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
